{
  "name": "stayte",
  "version": "0.1.1",
  "description": "The persistent state library for the modern web",
  "license": "MIT",
  "author": {
    "name": "Arthur tinseau",
    "email": "arthurtinseau@live.fr",
    "url": "https://www.linkedin.com/in/arthur-tinseau"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atinseau/stayte.git",
    "directory": "packages/stayte"
  },
  "homepage": "https://stayte.vercel.app",
  "keywords": [
    "react",
    "nextjs",
    "query",
    "cookies",
    "local-storage",
    "session-storage",
    "state-management",
    "persistent-state",
    "stayte",
    "zod",
    "cache",
    "validation"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "default": "./dist/react.js"
    }
  },
  "files": [
    "dist",
    "patches",
    "scripts/patch.js",
    "tsconfig.json"
  ],
  "scripts": {
    "postinstall": "node scripts/patch.js",
    "dev": "rollup --config rollup.config.mjs --watch",
    "build": "rm -rf dist && rollup --config rollup.config.mjs",
    "clean": "rm -rf dist",
    "fclean": "rm -rf dist .turbo node_modules"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "15.3.0",
    "@rollup/plugin-typescript": "12.1.0",
    "npm-run-all": "4.1.5",
    "rollup": "4.22.4",
    "rollup-plugin-esbuild-minify": "1.1.2",
    "typescript": "5.6.2",
    "react": "18.3.1",
    "zod": "3.23.8"
  },
  "dependencies": {
    "cookie-es": "1.2.2",
    "fast-equals": "5.0.1"
  }
}
